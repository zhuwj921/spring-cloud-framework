<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cloud.auth.mapper.ResourceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.cloud.auth.entity.Resource">
        <result column="id" property="id" />
        <result column="create_by" property="createBy" />
        <result column="create_time" property="createTime" />
        <result column="is_deleted" property="deleted" />
        <result column="modified_by" property="modifiedBy" />
        <result column="modified_time" property="modifiedTime" />
        <result column="version" property="version" />
        <result column="resource_key" property="resourceKey" />
        <result column="resource_name" property="resourceName" />
        <result column="p_id" property="pId" />
        <result column="remark" property="remark" />
        <result column="resource_type" property="resourceType" />
        <result column="url" property="url" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        create_by,
        create_time,
        is_deleted,
        modified_by,
        modified_time,
        version,
        resource_key, resource_name, p_id, remark, resource_type, url
    </sql>

</mapper>
